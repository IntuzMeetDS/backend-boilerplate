{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "dist/start.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/start.ts",
    "build": "tsc",
    "start": "node dist/start.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "keywords": [
    "fastify",
    "typescript",
    "boilerplate",
    "sequelize",
    "postgresql",
    "rest-api",
    "backend"<% if (redis) { %>,
    "redis"<% } %>
  ],
  "author": "<%= author %>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"<% if (redis) { %>,
    "ioredis": "^5.3.0"<% } %>
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.3.2",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
