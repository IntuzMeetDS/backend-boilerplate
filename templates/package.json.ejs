{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "dist/start.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/start.ts",
    "build": "tsc",
    "start": "node dist/start.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "node -r dotenv/config node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:undo": "node -r dotenv/config node_modules/.bin/sequelize-cli db:migrate:undo",
    "db:seed": "node -r dotenv/config node_modules/.bin/sequelize-cli db:seed:all"
  },
  "keywords": [
    "fastify",
    "typescript",
    "boilerplate",
    "sequelize",
    "postgresql",
    "rest-api",
    "backend"<% if (redis) { %>,
    "redis"<% } %>
  ],
  "author": "<%= author %>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "dotenv": "",
    "fastify": "",
    "moment": "",
    "pg": "",
    "pg-hstore": "",
    "sequelize": ""<% if (redis) { %>,
    "ioredis": ""<% } %>
  },
  "devDependencies": {
    "@types/node": "",
    "@typescript-eslint/eslint-plugin": "",
    "@typescript-eslint/parser": "",
    "eslint": "",
    "pino-pretty": "",
    "prettier": "",
    "sequelize-cli": "",
    "tsx": "",
    "typescript": ""
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
